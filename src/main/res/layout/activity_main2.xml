<TableLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools" android:layout_width="fill_parent" android:layout_height="fill_parent" tools:context=".MainActivity2">
        <TableRow
            android:layout_marginTop="30dp"
            android:id="@+id/ll2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp">
            <TextView
                android:id="@+id/fnama"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Nama Lengkap : "
                android:fontFamily="@font/poppinsbold"
                android:textSize="14dp"
                android:paddingLeft="10dp"
                android:paddingTop="5dp"/>
            <TextView
                android:id="@+id/fnama1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppinsbold"
                android:textSize="14dp"
                android:paddingLeft="10dp"
                android:paddingTop="5dp"/>
        </TableRow>
        <TableRow
            android:layout_marginTop="5dp"
            android:id="@+id/ll3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp">
            <TextView
                android:id="@+id/ftempat"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Tempat Lahir      : "
                android:fontFamily="@font/poppinsbold"
                android:textSize="14dp"
                android:paddingLeft="10dp"
                android:paddingTop="5dp"/>
            <TextView
                android:id="@+id/ftempat1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppinsbold"
                android:textSize="14dp"
                android:paddingLeft="10dp"
                android:paddingTop="5dp"/>
        </TableRow>

        <TableRow
            android:layout_marginTop="5dp"
            android:id="@+id/ll4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp">
            <TextView
                android:id="@+id/flahir"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Tanggal Lahir     : "
                android:fontFamily="@font/poppinsbold"
                android:textSize="14dp"
                android:paddingLeft="10dp"
                android:paddingTop="5dp"/>
            <TextView
                android:id="@+id/flahir1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/poppinsbold"
                android:textSize="14dp"
                android:paddingLeft="10dp"
                android:paddingTop="5dp"/>
        </TableRow>
    </TableLayout>
